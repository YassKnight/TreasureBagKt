<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.treasurebagkt">

    <application
        android:allowBackup="true"
        android:fullBackupOnly="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TreasureBagKt">


        <activity
            android:name=".module.main.MainActivity"
            android:exported="true">

        </activity>

        <!--        测试ManiFest配置界面-->
        <activity
            android:name=".test_module.module.testMani.TestManiFestActivity"
            android:exported="true" />
        <!--        菜单主页-->
        <activity
            android:name="com.example.treasurebagkt.test_module.module.main.MainMenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--        菜单栏界面-->
        <activity
            android:name=".test_module.module.actionBar.ActionBarActivity"
            android:exported="true" />
        <!--        打开浏览器自定义tab-->
        <activity
            android:name=".test_module.module.customWeb.CustomWebTabActivity"
            android:exported="true" />
    </application>

</manifest>